{
  "name": "o1js",
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-merge-modules"],
  // https://typedoc.org/options
  "tsconfig": "tsconfig.web.json",
  "out": "o1js-reference",
  "cleanOutputDir": true,
  "githubPages": false,
  "exclude": ["dist/**/*", "src/mina-signer/**/*", "src/bindings/**/*", "src/examples/**/*", "src/lib/provable/+(core|test)/**/*", "src/**/*(test|unit-test).ts"],
  "entryPoints": [
    "src/index.ts",
    "src/lib/provable/field.ts",
    "src/lib/provable/bool.ts",
    "src/lib/provable/group.ts",
    "src/lib/provable/int.ts",
    "src/lib/provable/scalar.ts",
    "src/lib/provable/merkle-list.ts",
    "src/lib/provable/merkle-map.ts",
    "src/lib/provable/merkle-tree.ts",
  ],
  "entryPointStrategy": "resolve",
  "excludeInternal": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeReferences": true,


  "fileExtension": ".mdx",
  "hidePageHeader": true,
  "hidePageTitle": true,
  "hideBreadcrumbs": true,
  "excludeGroups": true,
  "useCodeBlocks": true,
  "parametersFormat": "list",
  "propertiesFormat": "list",
  "typeDeclarationFormat": "list",
  "indexFormat": "table",
  "entryFileName": "Overview.mdx",

  "textContentMappings": {
    "breadcrumbs.home": "API",
    "kind.typeAlias.singular": "Type",
    "kind.typeAlias.plural": "Types",
  }
}
